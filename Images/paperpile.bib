  pages    = "448--459",
  month    =  "1~" # may,
  year     =  2018,
  keywords = "consens;Consensus",
  language = "en",
  issn     = "0027-8874, 1460-2105",
  pmid     = "29342309",
  doi      = "10.1093/jnci/djx243",
  pmc      = "PMC6279371"
}

