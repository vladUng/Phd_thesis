@ARTICLE{Chen2018-db,
  title    = "{GSAE}: an autoencoder with embedded gene-set nodes for genomics
              functional characterization",
  author   = "Chen, Hung-I Harry and Chiu, Yu-Chiao and Zhang, Tinghe and
